{
  "routes": [
    {
      "src": "/",
      "dest": "/login.html",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/index",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200,
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    }
  ],
  "trailingSlash": false
}
